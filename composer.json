{
  "name": "shibly/password-notify",
  "description": "Laravel package to notify user of password changes.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "shibly\\PasswordNotify\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "shibly\\PasswordNotify\\PasswordNotifyServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^11.0|^12.0",
    "illuminate/database": "^11.0|^12.0",
    "illuminate/notifications": "^11.0|^12.0",
    "illuminate/bus": "^11.0|^12.0",
    "illuminate/mail": "^11.0|^12.0"
  }
}
